TRISB1 = 1;
TRISB0 = 0;


RB0 = 0;


_calc_timer0_prescaler(brate, &prescaler_value, &prescaler_bits, &vw_tmr0_value);

if (prescaler_value > 1)
{

OPTION_REG &= 0xF0;
OPTION_REG |= prescaler_bits;
}

T0CS = 0;
TMR0 = (2 - vw_tmr0_value);

TMR0IF = 0;
TMR0IE = 1;
GIE = PEIE = 1;
}

