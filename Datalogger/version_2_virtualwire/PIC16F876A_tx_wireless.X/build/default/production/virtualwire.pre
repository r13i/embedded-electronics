for (pindex = 0; pindex < 8; pindex++)
{
*prescaler_value = prescaler_factor[pindex];
aux_tmr0 = 8000000 / (4 * (*prescaler_value) * brate * 8);

if (aux_tmr0 < 256)
{
*k_tmr0 = aux_tmr0;
if (pindex > 0)
{
*prescaler_bits = pindex - 1;
}
return 1;
}
}


return 0;
}

void vw_setup(uint16_t brate)
