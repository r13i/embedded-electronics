vw_rx_enabled = 1;
vw_rx_enabled_last = 1;
}

void vw_isr_tmr0(void)
{
TMR0 += -vw_tmr0_value + 4;
TMR0IF = 0;

if (vw_rx_enabled && !vw_tx_enabled)
vw_rx_sample = RB1;



if (vw_tx_enabled && vw_tx_sample++ == 0)
{
if (vw_tx_header)
{
RB0 = (vw_tx_buf_header[vw_tx_index] & (1 << vw_tx_bit++)) ? 1 : 0;
if (vw_tx_bit >= 6)
{
vw_tx_bit = 0;
vw_tx_index++;
}

if (vw_tx_index >= 8)
{

vw_tx_header = 0;
vw_tx_index = 0;
}
}
else
{




if (vw_tx_index >= vw_tx_len)
{
vw_tx_stop();
vw_rx_enabled = vw_rx_enabled_last;
}
else
{
RB0 = (vw_tx_buf[vw_tx_index] & (1 << vw_tx_bit++)) ? 1 : 0;
if (vw_tx_bit >= 6)
{
vw_tx_bit = 0;
vw_tx_index++;
}
}
}

