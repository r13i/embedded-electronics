boolean vw_recv(uint8_t *buf, uint8_t *len)
{
uint8_t rxlen;


if (!vw_rx_done)
return 0;



rxlen = vw_rx_len - 3;


if (*len > rxlen)
*len = rxlen;
memcpy(buf, vw_tx_buf + 1, *len);

vw_rx_done = 0;


return (vw_crc(vw_tx_buf, vw_rx_len) == 0xf0b8);
}

